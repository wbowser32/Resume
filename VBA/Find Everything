        Wholesale1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableWholeR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Recycled1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableRFR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Minority1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableMCR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
       
        Worksheets("Sheet2").Range("C5").Value = Sku1
        Worksheets("Sheet2").Range("C6").Value = Model1
        Worksheets("Sheet2").Range("C7").Value = Po1
