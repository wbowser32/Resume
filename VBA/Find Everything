Application.ScreenUpdating = False

Set Sku = Worksheets("Sheet1").Range("A:AK").Find("SKU Number")
Set Model = Worksheets("Sheet1").Range("A:AK").Find("Model")
Set Po = Worksheets("Sheet1").Range("A:AK").Find("PO Cost")
Set Net = Worksheets("Sheet1").Range("A:AK").Find("Net Cost")
Set Div = Worksheets("Sheet1").Range("A:AK").Find("Div")
Set DivName = Worksheets("Sheet1").Range("A:AK").Find("Div Name")
Set Dept = Worksheets("Sheet1").Range("A:AK").Find("Dept")
Set DeptName = Worksheets("Sheet1").Range("A:AK").Find("Dept Name")
Set Class = Worksheets("Sheet1").Range("A:AK").Find("Class")
Set ClassName = Worksheets("Sheet1").Range("A:AK").Find("Class Name")
Set SkuDesc = Worksheets("Sheet1").Range("A:AK").Find("SKU Desc")
Set PoCost = Worksheets("Sheet1").Range("A:AK").Find("PO Cost Change")
Set List = Worksheets("Sheet1").Range("A:AK").Find("List Price")
Set Uom = Worksheets("Sheet1").Range("A:AK").Find("UOM")
Set Uomq = Worksheets("Sheet1").Range("A:AK").Find("UOMQ")
Set Carton = Worksheets("Sheet1").Range("A:AK").Find("Carton QTY")
Set Weight = Worksheets("Sheet1").Range("A:AK").Find("Weight")
Set Length = Worksheets("Sheet1").Range("A:AK").Find("Length")
Set Height = Worksheets("Sheet1").Range("A:AK").Find("Height")
Set Width = Worksheets("Sheet1").Range("A:AK").Find("Width")
Set UPC = Worksheets("Sheet1").Range("A:AK").Find("UPC")
Set VendorNum = Worksheets("Sheet1").Range("A:AK").Find("Vendor Num")
Set Vendor = Worksheets("Sheet1").Range("A:AK").Find("Vendor")
