        Uom1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableUOMR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Uomq1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableUOMQR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Carton1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableCQtR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Weight1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableWR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Length1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableLR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Height1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableHR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
