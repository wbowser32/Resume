Set FcOh = Worksheets("Sheet1").Range("A:AK").Find("FC OH")
Set Dropship = Worksheets("Sheet1").Range("A:AK").Find("Drop Ship IND")
Set Wholesale = Worksheets("Sheet1").Range("A:AK").Find("Wholesaler IND")
Set Recycled = Worksheets("Sheet1").Range("A:AK").Find("Recycled Flag")
Set Minority = Worksheets("Sheet1").Range("A:AK").Find("Minority Code")
Set SkuR = Sku.EntireColumn
Set ModelR = Model.EntireColumn
Set PoR = Po.EntireColumn
Set NetR = Net.EntireColumn
Set DivR = Div.EntireColumn
Set DivNameR = DivName.EntireColumn
Set DeptR = Dept.EntireColumn
Set DeptNameR = DeptName.EntireColumn
Set ClassR = Class.EntireColumn
Set ClassNameR = ClassName.EntireColumn
Set SkuDescR = SkuDesc.EntireColumn
Set PoCostR = PoCost.EntireColumn
Set ListR = List.EntireColumn
Set UomR = Uom.EntireColumn
Set UomqR = Uomq.EntireColumn
Set CartonR = Carton.EntireColumn
Set WeightR = Weight.EntireColumn
Set LengthR = Length.EntireColumn
Set HeightR = Height.EntireColumn
Set WidthR = Width.EntireColumn
Set UPCR = UPC.EntireColumn
Set VendorNumR = VendorNum.EntireColumn
Set VendorR = Vendor.EntireColumn
Set SourceINDR = SourceIND.EntireColumn
Set FcOhR = FcOh.EntireColumn
Set DropshipR = Dropship.EntireColumn
Set WholesaleR = Wholesale.EntireColumn
Set RecycledR = Recycled.EntireColumn
Set MinorityR = Minority.EntireColumn
Variable = SkuR.Address(External:=True)
VariableM = ModelR.Address(External:=True)
VariableP = PoR.Address(External:=True)
VariableN = NetR.Address(External:=True)
VariableDiv = DivR.Address(External:=True)
VariableDivN = DivNameR.Address(External:=True)
VariableDept = DeptR.Address(External:=True)
VariableDeptN = DeptNameR.Address(External:=True)
VariableClass = ClassR.Address(External:=True)
VariableClassN = ClassNameR.Address(External:=True)
VariableSkuD = SkuDescR.Address(External:=True)
VariablePoC = PoCostR.Address(External:=True)
VariableLP = ListR.Address(External:=True)
VariableUOM = UomR.Address(External:=True)
VariableUOMQ = UomqR.Address(External:=True)
VariableCQt = CartonR.Address(External:=True)
VariableW = WeightR.Address(External:=True)
VariableL = LengthR.Address(External:=True)
VariableH = HeightR.Address(External:=True)
VariableWid = WidthR.Address(External:=True)
VariableUPC = UPCR.Address(External:=True)
VariableVN = VendorNumR.Address(External:=True)
VariableV = VendorR.Address(External:=True)
VariableSIND = SourceINDR.Address(External:=True)
VariableFcOh = FcOhR.Address(External:=True)
VariableDrop = DropshipR.Address(External:=True)
VariableWhole = WholesaleR.Address(External:=True)
VariableRF = RecycledR.Address(External:=True)
VariableMC = MinorityR.Address(External:=True)
Set VariableR = Range(Variable)
Set VariableMR = Range(VariableM)
Set VariablePoR = Range(VariableP)
Set VariableNetR = Range(VariableN)
Set VariableDivR = Range(VariableDiv)
Set VariableDivNR = Range(VariableDivN)
Set VariableDeptR = Range(VariableDept)
Set VariableDeptNR = Range(VariableDeptN)
Set VariableClassR = Range(VariableClass)
Set VariableClassNR = Range(VariableClassN)
Set VariableSkuDR = Range(VariableSkuD)
Set VariablePoCR = Range(VariablePoC)
Set VariableLPR = Range(VariableLP)
Set VariableUOMR = Range(VariableUOM)
Set VariableUOMQR = Range(VariableUOMQ)
Set VariableCQtR = Range(VariableCQt)
Set VariableWR = Range(VariableW)
Set VariableLR = Range(VariableL)
Set VariableHR = Range(VariableH)
Set VariableWidR = Range(VariableWid)
Set VariableUPCR = Range(VariableUPC)
Set VariableVNR = Range(VariableVN)
Set VariableVR = Range(VariableV)
Set VariableSINDR = Range(VariableSIND)
Set VariableFcOhR = Range(VariableFcOh)
Set VariableDropR = Range(VariableDrop)
Set VariableWholeR = Range(VariableWhole)
Set VariableRFR = Range(VariableRF)
Set VariableMCR = Range(VariableMC)

On Error GoTo Boi


ActiveSheet.Pictures.Delete


        Worksheets("Sheet1").Select
        ModelR.Select
        Selection.Replace What:="\", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:="/", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:=".", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:="-", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:="_", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:=",", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Selection.Replace What:=" ", Replacement:="", LookAt:=xlPart, _
        SearchOrder:=xlByRows, MatchCase:=False, SearchFormat:=False, _
        ReplaceFormat:=False
        Worksheets("Sheet2").Select

    If IsEmpty(Worksheets("Sheet2").Range("C1").Value) = True And IsEmpty(Worksheets("Sheet2").Range("C2").Value) = True Then
        
        Worksheets("Sheet2").Range("C5:C34").Select
        Selection.ClearContents
        Worksheets("Sheet2").Range("I5").Select
        MsgBox "Cell C1 & C2 are Blank. Please fill it in one of these cells."
        
    
    ElseIf IsEmpty(Worksheets("Sheet2").Range("C1").Value) = False And IsEmpty(Worksheets("Sheet2").Range("C2").Value) = True Then

    
    If Application.WorksheetFunction.CountIf(VariableR, Worksheets("Sheet2").Range("C1").Value) > 0 = True Then
        
        
        Sku1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Model1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableMR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Po1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariablePoR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Net1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableNetR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
        Div1 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDivR, Application.Match((Worksheets("Sheet2").Range("C1")), VariableR, 0)), 0)
