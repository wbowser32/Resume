        Worksheets("Sheet2").Range("C12").Value = DeptName1
        Worksheets("Sheet2").Range("C13").Value = Class1
        Worksheets("Sheet2").Range("C14").Value = ClassName1
        Worksheets("Sheet2").Range("C15").Value = SkuDesc1
        Worksheets("Sheet2").Range("C16").Value = PoCost1
        Worksheets("Sheet2").Range("C17").Value = List1
        Worksheets("Sheet2").Range("C18").Value = Uom1
        Worksheets("Sheet2").Range("C19").Value = Uomq1
        Worksheets("Sheet2").Range("C20").Value = Carton1
        Worksheets("Sheet2").Range("C21").Value = Weight1
        Worksheets("Sheet2").Range("C22").Value = Length1
        Worksheets("Sheet2").Range("C23").Value = Height1
        Worksheets("Sheet2").Range("C24").Value = Width1
        Worksheets("Sheet2").Range("C25").Value = UPC1
        Worksheets("Sheet2").Range("C26").Value = VendorNum1
        Worksheets("Sheet2").Range("C27").Value = Vendor1
        Worksheets("Sheet2").Range("C28").Value = SourceIND1
        Worksheets("Sheet2").Range("C29").Value = FcOh1
        Worksheets("Sheet2").Range("C30").Value = Dropship1
        Worksheets("Sheet2").Range("C31").Value = Wholesale1
        Worksheets("Sheet2").Range("C32").Value = Recycled1
        Worksheets("Sheet2").Range("C33").Value = Minority1
        Worksheets("Sheet2").Range("C2").Value = Model1
        Worksheets("Sheet2").Range("C34").Select
        Selection.ClearContents

    Else:
        
        Worksheets("Sheet2").Range("C5:C34").Select
        Selection.ClearContents
        Range("I5").Select
        MsgBox ("This number is not found in the file. Please check to see if it it is correct")
        
        
        End If
        
    ElseIf IsEmpty(Worksheets("Sheet2").Range("C1").Value) = True And IsEmpty(Worksheets("Sheet2").Range("C2").Value) = False Then
    
    If Application.WorksheetFunction.CountIf(ModelR, removeSpecial(Worksheets("Sheet2").Range("C2"))) > 0 = True Then
    
        Worksheets("Sheet2").Range("C2").Value = removeSpecial(Worksheets("Sheet2").Range("C2"))
        
        Sku2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Model2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableMR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Po2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariablePoR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Net2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableNetR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Div2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDivR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        DivName2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDivNR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Dept2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDeptR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        DeptName2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDeptNR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Class2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableClassR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        ClassName2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableClassNR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        SkuDesc2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableSkuDR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        PoCost2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariablePoCR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        List2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableLPR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Uom2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableUOMR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Uomq2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableUOMQR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Carton2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableCQtR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Weight2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableWR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Length2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableLR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Height2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableHR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Width2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableWidR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        UPC2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableUPCR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        VendorNum2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableVNR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Vendor2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableVR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        SourceIND2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableSINDR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        FcOh2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableFcOhR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Dropship2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableDropR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Wholesale2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableWholeR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Recycled2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableRFR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        Minority2 = Application.WorksheetFunction.IfError(Application.WorksheetFunction.Index(VariableMCR, Application.Match(Worksheets("Sheet2").Range("C2").Value, VariableMR, 0)), 0)
        
        
